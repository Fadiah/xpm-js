{
  "name": "xpm",
  "version": "0.1.1",
  "description": "The xPack package manager command line tool",
  "main": "./index.js",
  "directories": {
    "bin": "./bin",
    "lib": "./lib",
    "test": "./test"
  },
  "bin": {
    "xpm": "./bin/xpm.js"
  },
  "scripts": {
    "test": "standard && npm run test-tap -s",
    "test-coverage": "tap --coverage --reporter=classic --timeout 600 \"test/tap/*.js\"",
    "test-tap": "tap --reporter=classic --timeout 300 \"test/tap/*.js\"",
    "tap": "tap --reporter=spec --timeout 300",
    "fix": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xpack/xpm-js.git"
  },
  "keywords": [
    "xpack",
    "package",
    "manager",
    "npm"
  ],
  "author": {
    "name": "Liviu Ionescu",
    "email": "ilg@livius.net",
    "url": "http://liviusdotnet.wordpress.com"
  },
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xpack/xpm-js/issues"
  },
  "homepage": "https://github.com/xpack/xpm-js",
  "dependencies": {
    "@ilg/cli-start-options": "~0.1.0",
    "shopify-liquid": "~1.7.7"
  },
  "devDependencies": {
    "mkdirp": "~0.5.1",
    "rimraf": "~2.6.1",
    "standard": "~10.0.1",
    "tar": "~2.2.1",
    "tap": "~10.3.1"
  },
  "peerDependencies": {},
  "bundledDependencies": [],
  "optionalDependencies": {},
  "engines": {
    "node": ">7.7.0"
  }
}
